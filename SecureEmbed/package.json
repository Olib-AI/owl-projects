{
  "name": "@olib-ai/secure-embed",
  "version": "2.0.0",
  "description": "Securely embed third-party scripts without hardcoding credentials. Uses Service Worker + WebAssembly encryption for runtime credential injection with maximum security.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/SecureEmbed.d.ts",
      "import": "./dist/react/SecureEmbed.js",
      "default": "./dist/react/SecureEmbed.js"
    },
    "./service-worker": {
      "import": "./dist/secure-sw.min.js",
      "default": "./dist/secure-sw.min.js"
    },
    "./wasm": {
      "import": "./wasm/build/crypto-core.wasm",
      "default": "./wasm/build/crypto-core.wasm"
    }
  },
  "bin": {
    "secure-embed": "./dist/cli.js"
  },
  "files": [
    "dist",
    "wasm/build/*.wasm",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:wasm && tsc && npm run build:sw && npm run build:minify",
    "build:wasm": "cd wasm && npx asc assembly/index.ts --target release",
    "build:sw": "esbuild src/secure-core/secure-service-worker.ts --bundle --format=iife --platform=browser --target=es2022 --outfile=dist/secure-sw.js --external:*.wasm --define:DEBUG=false",
    "build:minify": "esbuild dist/secure-sw.js --minify --mangle-props='^_' --keep-names=false --outfile=dist/secure-sw.min.js",
    "dev": "tsc --watch",
    "demo": "node examples/serve.js",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "test:wasm": "cd wasm && npx asc assembly/index.ts --target debug"
  },
  "keywords": [
    "security",
    "embed",
    "service-worker",
    "encryption",
    "credentials",
    "third-party",
    "webassembly",
    "wasm",
    "obfuscation",
    "intercom",
    "analytics",
    "chat-widget"
  ],
  "author": "Olib AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Olib-AI/owl-projects.git",
    "directory": "SecureEmbed"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "assemblyscript": "^0.27.0",
    "esbuild": "^0.24.0",
    "typescript": "^5.3.0"
  },
  "sideEffects": false
}
